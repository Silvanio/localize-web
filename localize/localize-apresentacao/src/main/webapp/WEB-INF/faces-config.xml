<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">


	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<!-- Regras de Navegacao Fornecedor -->
	
	<navigation-rule>
		<navigation-case>
			<from-outcome>ManterFornecedorController/tipoServico</from-outcome>
			<to-view-id>/paginas/tipoServico.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<navigation-case>
			<from-outcome>ManterFornecedorController/inicial</from-outcome>
			<to-view-id>/paginas/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<navigation-case>
			<from-outcome>ManterFornecedorController/localizacao</from-outcome>
			<to-view-id>/paginas/localizacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>message</var>
		</resource-bundle>
	</application>
	
</faces-config>
